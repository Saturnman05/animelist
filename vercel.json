{
  "version": 2,
  "projects": [
    {
      "name": "front",
      "src": "front",
      "buildCommand": "npm install && npm run build",
      "outputDirectory": "front/dist/<nombre-app>",
      "routes": [
        {
          "src": "/(.*)",
          "dest": "/front/$1",
          "status": 200
        }
      ]
    },
    {
      "name": "back",
      "src": "back",
      "buildCommand": "pip install -r requirements.txt",
      "outputDirectory": "back",
      "routes": [
        {
          "src": "/(.*)",
          "dest": "/back/main.py",
          "status": 200
        }
      ]
    }
  ]
}
